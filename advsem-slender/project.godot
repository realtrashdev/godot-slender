config_version=5

[application]

config/name="advsem-slender"
run/main_scene="uid://c3f6y28ur3tma"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.svg"

[aseprite_importers]

aseprite_executable_path="C:/Program Files (x86)/Steam/steamapps/common/Aseprite/Aseprite.exe"

[autoload]

GameConfig="*res://scripts/globals/game_config.gd"
SaveManager="*res://scripts/globals/save_manager.gd"
Settings="*res://scripts/globals/settings.gd"
Progression="*res://scripts/globals/progression.gd"
Signals="*res://scripts/globals/signals.gd"
ScreenColorCanvas="*res://scenes/ui/canvases/screen_color_canvas.tscn"
EnemyOverview="*res://scenes/ui/canvases/enemies_overview.tscn"

[debug]

gdscript/warnings/unused_parameter=0
gdscript/warnings/narrowing_conversion=0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/size/initial_position_type=3
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/SmoothScroll/plugin.cfg", "res://addons/nklbdev.aseprite_importers/plugin.cfg")

[global_group]

Player="The player"
PageLocation="A place where a page can generate."
Tile=""
Grass=""
Path=""
SpawnMarkers="Pre placed markers some 3D enemies use for spawning."
Pathfinder="Enemies that pathfind to the player. Uses a tick system to prevent lag and stagger enemy pathfinding updates."
Slows="Ground that slows the player down when walking on it."
ComponentEnemy="All enemies that act as components rather than physical enemies in the scene."

